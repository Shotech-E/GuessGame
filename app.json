{
  "expo": {
    "name": "GuessGame",
    "slug": "GuessGame",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/gIcon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/ac.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#084e0e"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-splash-screen",
      "expo-font"
    ]
  }
}